<div class="container full-screen">
<div class="banner-title"><h1>ELEPHANT<small> &amp; </small>CASTLE</h1></div>
  <% @posts.each do |post| %>
  <h3><%= post.title %> <small><%= link_to "Edit", edit_post_path(post) %></small> </h3>
  <p><%= post.content.html_safe %></p>
  <% end %>
  <%= link_to "New post", new_post_path %>
</div>
